{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "module": "ES6",
    "target": "es2022",
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "moduleResolution":"node",
  }
}